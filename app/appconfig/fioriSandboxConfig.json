{
  "services": {
    "LaunchPage": {
      "adapter": {
        "config": {
          "catalogs": [],
          "groups": [
            {
              "id": "Procurement",
              "title": "AI-Powered Procurement Assistant",
              "isPreset": true,
              "isVisible": true,
              "isGroupLocked": false,
              "tiles": [
                {
                  "id": "ProcurementAssistant",
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "AI Procurement Assistant",
                    "subtitle": "Modern Fiori UI5 Interface",
                    "icon": "sap-icon://business-objects-experience",
                    "targetURL": "#Procurement-modern"
                  }
                },
                {
                  "id": "ProcurementLegacy",
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "Legacy Procurement UI",
                    "subtitle": "Original Static HTML Interface",
                    "icon": "sap-icon://legacy",
                    "targetURL": "#Procurement-assistant"
                  }
                },
                {
                  "id": "BackendTesting",
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "Backend Testing Console",
                    "subtitle": "API & Database Testing",
                    "icon": "sap-icon://lab",
                    "targetURL": "#Procurement-testing"
                  }
                }
              ]
            }
          ]
        }
      }
    },
    "NavTargetResolution": {
      "config": {
        "enableClientSideTargetResolution": true
      }
    },
    "ClientSideTargetResolution": {
      "adapter": {
        "config": {
          "inbounds": {
            "ProcurementAssistant": {
              "semanticObject": "Procurement",
              "action": "assistant",
              "title": "Legacy Procurement Assistant",
              "signature": {
                "parameters": {},
                "additionalParameters": "allowed"
              },
              "resolutionResult": {
                "applicationType": "URL",
                "url": "procurement-assistant/webapp/index.html"
              }
            },
            "ProcurementModern": {
              "semanticObject": "Procurement",
              "action": "modern",
              "title": "Modern Procurement UI",
              "signature": {
                "parameters": {},
                "additionalParameters": "allowed"
              },
              "resolutionResult": {
                "applicationType": "URL",
                "url": "procurement-assistant/webapp/working-modern.html"
              }
            },
            "ProcurementTesting": {
              "semanticObject": "Procurement",
              "action": "testing",
              "title": "Backend Testing Console",
              "signature": {
                "parameters": {},
                "additionalParameters": "allowed"
              },
              "resolutionResult": {
                "applicationType": "URL",
                "url": "procurement-assistant/webapp/backend-testing.html"
              }
            }
          }
        }
      }
    }
  }
}
