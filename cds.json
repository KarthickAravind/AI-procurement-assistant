{
  "build": {
    "target": "."
  },
  "requires": {
    "db": {
      "kind": "sqlite",
      "credentials": {
        "url": "db.sqlite"
      }
    }
  },
  "folders": {
    "db": "db/",
    "srv": "srv/",
    "app": "app/"
  },
  "cdsc": {
    "docs": false
  },
  "features": {
    "fetch_csrf": true
  }
}
